<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:VM="clr-namespace:OnlineCinema.ViewModel"
                    xmlns:View="clr-namespace:OnlineCinema.View"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate DataType="{x:Type VM:MainPageVM}">
        <View:MainPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type VM:SelectedFilmPageVM}">
        <View:SelectedFilmPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type VM:BuyMoviePageVM}">
        <View:BuyMoviePage/>
    </DataTemplate>
</ResourceDictionary>